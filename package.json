    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.1.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
